OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/CLLocationManager-blocks" -isystem "${PODS_ROOT}/Headers/Public/Foursquare-iOS-API"
OTHER_LDFLAGS = $(inherited) -ObjC -l"CLLocationManager-blocks" -l"Foursquare-iOS-API" -framework "CoreLocation" -framework "MobileCoreServices"
PODS_ROOT = ${SRCROOT}/Pods